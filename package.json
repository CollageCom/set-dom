{
  "name": "set-dom",
  "description": "Lightweight dom diffing library with plain old html.",
  "version": "2.0.1",
  "author": "Dylan Piercey <pierceydylan@gmail.com>",
  "devDependencies": {
    "browserify": "^13.1.0",
    "jsdom": "^9.5.0",
    "jsdom-global": "^2.1.0",
    "snazzy": "^5.0.0",
    "standard": "^8.0.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "uglify-js": "^2.7.3"
  },
  "keywords": [
    "diff",
    "dom",
    "hyperx",
    "jsx",
    "react",
    "render",
    "set-dom",
    "tusk",
    "update",
    "virtual"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/DylanPiercey/set-dom"
  },
  "scripts": {
    "build": "browserify -s setDOM ./lib/index.js | uglifyjs -c warnings=false -m > ./dist/set-dom.js",
    "lint": "standard --verbose | snazzy",
    "test": "npm run lint && tape ./**/*.test.js | tap-spec"
  },
  "standard": {
    "ignore": [
      "dist"
    ]
  }
}
